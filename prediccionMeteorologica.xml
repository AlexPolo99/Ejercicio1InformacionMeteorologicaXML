<?xml version ="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/css" href="style.css"?>
<!DOCTYPE prediccion[
<!ELEMENT prediccion (datosPoblacion, prediccionTiempo)>
    <!ELEMENT datosPoblacion (nombrePoblacion, nombreProvincia, nombreLocalidad)>
        <!ELEMENT nombrePoblacion (#PCDATA)>
        <!ELEMENT nombreProvincia (#PCDATA)>
        <!ELEMENT nombreLocalidad (#PCDATA)>
    <!ELEMENT prediccionTiempo (fecha, temperaturaMaxima, temperaturaMinima, probabilidadLluvia)>
        <!ELEMENT fecha (#PCDATA)>
        <!ELEMENT temperaturaMaxima (#PCDATA)>
        <!ELEMENT temperaturaMinima (#PCDATA)>
        <!ELEMENT probabilidadLluvia (#PCDATA)>
]>
<prediccion>
    <datosPoblacion>
        <nombrePoblacion></nombrePoblacion>
        <nombreProvincia></nombreProvincia>
        <nombreLocalidad></nombreLocalidad>
    </datosPoblacion>    
    <prediccionTiempo>
        <fecha></fecha>
        <temperaturaMaxima></temperaturaMaxima>
        <temperaturaMinima></temperaturaMinima>
        <probabilidadLluvia></probabilidadLluvia>
    </prediccionTiempo>
</prediccion>